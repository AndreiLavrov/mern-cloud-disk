{
    "root": true,
    "parserOptions": {
        "ecmaVersion": 12
    },
    "extends": [
        "google",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "import/prefer-default-export": 0,
        "import/no-unresolved": 0,
        "jsx-a11y/click-events-have-key-events": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "no-shadow": 0,
        "no-nested-ternary": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "require-jsdoc" : 0
    }
}
